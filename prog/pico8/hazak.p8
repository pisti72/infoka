pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
x=20
y=20
function _draw()
 if btn(➡️) then x=x+1 end
 if btn(⬅️) then x=x-1 end
 if btn(⬇️) then y=y+1 end
 if btn(⬆️) then y=y-1 end
 cls(3)
 map()
 for j=0,4 do
  for i=0,4 do
  	spr(1,10+x*i,j*y+5,2,2)
  end
 end
end
