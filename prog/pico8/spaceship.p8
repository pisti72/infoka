pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
y=60; buls={}; f_rate=0
function _draw()
 cls()
 if(btn(⬆️) and y>0) y-=2
 if(btn(⬇️) and y<120) y+=2
 if btn(❎) and f_rate==0 then
  add(buls,{x=4,y=y})
  f_rate=10
 end
 if(f_rate>0)f_rate-=1
 print(#buls..","..f_rate)
 spr(1,0,y)
 for b in all(buls) do
  b.x+=5
  spr(2,b.x,b.y)
 end
end
