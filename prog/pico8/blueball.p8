end

function _draw()
 cls()
 if btn(⬅️) then
  x=x-1
 end
 if btn(➡️) then
  x=x+1
 end
 if btn(⬆️) then
  y=y-1
 end
 if btn(⬇️) then
  y=y+1
 end
 spr(1,x,y)
end
